<launch>

    <node pkg="odom_path_tools" type="odoms_to_paths_node" name="odoms_to_paths" output="screen">
        <param name="mode" value="path"/> 
        <param name="fixed_frame" value="map"/>
        <param name="topic_odom_filtered" value="/odometry/filtered"/>
